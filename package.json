    "csp-generator": "^1.0.2",
    "lib-jitsi-meet-dist": "^2.1.3",
    "lodash-es": "^4.17.15",
    "loglevel": "^1.7.0",
    "prompts": "^2.3.2",
    "protoframe": "^0.2.1",
    "puppeteer": "^5.3.0",
    "v-emoji-picker": "^2.3.0",
    "vue": "^2.6.12",
    "vue-class-component": "^7.2.6",
